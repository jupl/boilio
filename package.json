{
  "name": "Application",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^6.0.0 || ^7.0.0",
    "npm": "^3.0.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/common/create-store.js"
    ],
    "preset": "jest-react-native"
  },
  "scripts": {
    "start": "npm run server",
    "coverage": "npm run test -- --coverage",
    "coverage:watch": "npm run coverage -- --watch",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "server": "node node_modules/react-native/local-cli/cli.js start",
    "server:story": "storybook start -p 9001"
  },
  "dependencies": {
    "babel-preset-react-native": "^1.9.0",
    "react": "^15.3.2",
    "react-native": "^0.37.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-actions": "^0.13.0",
    "redux-saga": "^0.12.1",
    "remote-redux-devtools": "^0.5.3"
  },
  "devDependencies": {
    "@kadira/react-native-storybook": "^2.1.7",
    "babel-eslint": "^7.0.0",
    "enzyme": "^2.6.0",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "^0.0.3",
    "eslint": "^3.8.1",
    "eslint-config-jupl": "^0.4.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.6.0",
    "globule": "^1.1.0",
    "jest": "^17.0.0",
    "jest-react-native": "^17.0.0",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2"
  }
}
